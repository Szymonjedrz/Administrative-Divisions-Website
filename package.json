{
  "name": "demo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.10",
    "@mui/x-data-grid": "^6.14.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.46",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.5.0",
    "babel-loader": "^9.1.3",
    "body-parser": "^1.20.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "git": "^0.1.5",
    "mini-css-extract-plugin": "^2.7.6",
    "morgan": "^1.10.0",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "regenerator-runtime": "^0.13.11",
    "serialize-javascript": "^6.0.1",
    "style-loader": "^3.3.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.88.2",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/node": "^7.22.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.6.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "reactful": "^3.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint \"src/**/*.js\"",
    "dev:server": "cross-env NODE_PATH=./src nodemon --exec \"babel-node src/server/server.js\" --ignore .reactful.json --ignore public/",
    "dev:bundler": "webpack -w --mode=development",
    "build:react": "cross-env NODE_ENV=production webpack --progress --mode=production",
    "build:node": "babel src -d build --config-file ./babel-node.config.js --copy-files",
    "build:all": "npm install && npm run build:react && npm run build:node",
    "prod:start": "cross-env NODE_ENV=production NODE_PATH=./build pm2 start -i max build/server/server.js --update-env --name test-appProd",
    "prod:stop": "pm2 stop test-appProd",
    "prod:reload": "pm2 reload --update-env test-appProd",
    "prod:logs": "pm2 logs --update-env test-appProd",
    "verify-tests": "jest --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
